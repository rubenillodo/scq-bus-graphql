{
  "name": "scq-bus-graphql",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenillodo/scq-bus-graphql"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^10.0.0"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/src",
    "build": "tsc && cpy '**/*.graphql' '../dist/src' --cwd=src --parents",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md,graphql}\"",
    "clean": "rimraf dist",
    "types": "gql-gen",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.5.0",
    "apollo-server": "^2.14.2",
    "cross-fetch": "^3.0.4",
    "graphql": "^14.3.1",
    "graphql-import": "^0.7.1",
    "luxon": "^1.16.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.2.0",
    "@graphql-codegen/typescript": "^1.2.0",
    "@graphql-codegen/typescript-resolvers": "^1.2.0",
    "@types/cheerio": "^0.22.11",
    "@types/fs-extra": "^7.0.0",
    "@types/graphql": "^14.2.0",
    "@types/luxon": "^1.15.1",
    "cheerio": "^1.0.0-rc.3",
    "cpy-cli": "^2.0.0",
    "fs-extra": "^8.0.1",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.12",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  }
}
