{
  "name": "scq-bus-graphql",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenillodo/scq-bus-graphql"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^10.0.0"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/src",
    "build": "tsc && cpy '**/*.graphql' '../dist/src' --cwd=src --parents",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md,graphql}\"",
    "clean": "rimraf dist",
    "types": "gql-gen",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.4.0",
    "apollo-server": "^2.4.8",
    "cross-fetch": "^3.0.2",
    "graphql": "^14.2.1",
    "graphql-import": "^0.7.1",
    "luxon": "^1.12.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.1.1",
    "@graphql-codegen/typescript": "^1.1.1",
    "@graphql-codegen/typescript-resolvers": "^1.1.1",
    "@types/cheerio": "^0.22.11",
    "@types/fs-extra": "^5.0.5",
    "@types/graphql": "^14.2.0",
    "@types/luxon": "^1.12.0",
    "cheerio": "^1.0.0-rc.3",
    "cpy-cli": "^2.0.0",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.11",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
